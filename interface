# criando conexao
from faker import Faker 
import psycopg2
conn = psycopg2.connect(host="localhost",
        database="Interface",
        user="postgres",
        password="/@JK@bb78")
print("Conexão com o Banco de Dados aberta com sucesso!")
cur=conn.cursor() 
fake= Faker('pt_BR')
n=10
for i in range(n):
    codigo=i+10
    nome='produto_'+str(i+1)
    preco= fake.pyfloat(left_digits=3, right_digits=2, positive= True, 
                        min_value=5, max_value=1000)

print(preco)
print(nome)
comandoSql= """ INSERT INTO public."produto2"("codigo2", "nome2", "preco2") VALUES (%s, %s, %s)""" 
registro= (codigo, nome, preco)
cur.execute(comandoSql, registro)
conn.commit() 
print("Inserção realizada com sucesso!"); 
conn.close()